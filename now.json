{
  "version": 2,
  "builds": [
    { "src": "frontend/public/**/*", "use": "@now/static" },
    { "src": "backend/api/index.go", "use": "@now/go" }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "backend/api/index.go" },
    { "src": "/", "dest": "frontend/public/index.html" },
    { "src": "/(.*)", "dest": "frontend/public/$1" }
  ],
  "env": {}
}
