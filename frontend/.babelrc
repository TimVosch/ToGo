{
  "presets": [
    ["@babel/env", { "targets": { "esmodules": true } }],
    ["@babel/preset-typescript", { "jsxPragma": "h" }]
  ],
  "plugins": [
    ["@babel/plugin-transform-react-jsx", { "pragma": "h" }],
    ["@babel/plugin-proposal-class-properties"],
    ["@babel/plugin-proposal-optional-chaining"]
  ]
}
